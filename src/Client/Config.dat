// Pocz¹tkowe ustawienia graficzne
Display
{
	// Wybór urz¹dzenia graficznego.
	// Podaj indeks, np. 0, 1, 2 itd., jeœli masz wiêcej kart graficznych
	// Domyœlnie: DEFAULT.
	Adapter = DEFAULT
	// Format bufora g³êbokoœci i bufora szablonu.
	// Dopuszczalne: D16, D24X8, D24S8.
	// Jeœli program u¿ywa bufora szablonu, trzeba podaæ D24S8.
	// Jeœli nie u¿ywa, mo¿na podaæ dowolny.
	// Ten program nie u¿ywa bufora szablonu, ale D16 powoduje brzydkie linie na ³¹czeniach fragmentów terenu.
	// Domyœlnie: D24X8
	AutoDepthStencilFormat = D24X8
	// Liczba tylnych buforów: 0, 1, 2, 3.
	// 0 oznacza to samo, co 1.
	// Domyœlnie: 1
	BackBufferCount = 1
	// Format tylnego bufora.
	// Domyœlnie: A8R8G8B8
	BackBufferFormat = A8R8G8B8
	// Rozdzielczoœæ w pikselach.
	// Domyœlnie: odpowiednio 800 i 600.
	BackBufferWidth = 1024
	BackBufferHeight = 768
	// Czêstotliwoœæ odœwie¿ania w Hz.
	// W przypadku trybu okienkowego nie ma znaczenia.
	// Domyœlnie: 60
	RefreshRate = 60
	// Tryb pe³noekranowy.
	// true - pe³ny ekran, false - okno
	// Domyœlnie: false
	FullScreen = true
	// Rodzaj urz¹dzenia - HAL lub REF.
	// HAL - sterownik sprzêtowy
	// REF - sterownik referencyjny. Nie wybieraæ - dzia³a zbyt wolno!
	// Domyœlnie: HAL
	DeviceType = HAL
	// Czy mo¿na porzucaæ poprzedni¹ zawartoœæ bufora g³êbokoœci i szablonu.
	// Zale¿y od sposobu jego wykorzystania przez aplikacjê.
	// W przypadku tej aplikacji mo¿e i powinno byæ true.
	DiscardDepthStencil = true
	// Czy bufor g³êbokoœci i szablonu jest w³¹czony.
	// Zale¿y od sposobu jego wykorzystania przez aplikacjê.
	// W przypadku tej aplikacji musi byæ true.
	EnableAutoDepthStencil = true
	// Czy bufor ekranu ma siê nadawaæ do blokowania.
	// Zale¿y od sposobu jego wykorzystania przez aplikacjê.
	// W przypadku tej aplikacji mo¿e i powinno byæ false.
	LockableBackBuffer = false
	// Ustawienia antialiasingu.
	// Wy³¹czone. Nie sprawdzane, czy program dzia³a przy w³¹czonych.
	MultiSampleQuality = 0
	MultiSampleType = NONE
	// Sposób prezentacji klatki. Odpowiada za pionow¹ synchronizacjê (VSync).
	// Dopuszczalne wartoœci: IMMEDIATE, DEFAULT, ONE, TWO, THREE, FOUR.
	// IMMEDIATE - VSync wy³¹czone
	// DEFAULT - VSync w³¹czone
	// Domyœlnie: IMMEDIATE
	PresentationInverval = IMMEDIATE
	// Sposób prezentacji klatki.
	// Dopuszczalne wartoœci: DISCARD, FLIP, COPY
	// Domyœlnie: DISCARD
	SwapEffect = DISCARD
	// Sposób przetwarzania wierzcho³ków.
	// Dopuszczalne wartoœci: HARDWARE, MIXED, SOFTWARE, AUTO
	// Domyœlnie: AUTO
	// Mo¿esz spróbowaæ przestawiæ na SOFTWARE, jeœli program siê nie uruchamia i masz kartê zintegrowan¹ firmy Intel.
	VertexProcessing = AUTO
	// Tryb oczekiwania na pokazanie klatki.
	// Dopuszczalne wartoœci: None, Event, Lock
	// Domyœlnie: Event
	// None - brak oczekiwania, zalecane - pozwala uzyskaæ najwiêcej FPS
	// Event - wybierz, jeœli gra "szarpie"
	// Lock - wybierz, jeœli gra "szarpie", a Event nie pomaga
	FlushMode = Event
}

